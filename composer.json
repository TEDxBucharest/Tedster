{
    "name": "tedxbucharest/tedster",
    "description": "Facebook profile photo overlay",
    "keywords": ["ted", "tedx", "tedxbucharest", "bucharest", "facebook", "profile", "photo", "picture", "overlay", "tedster"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "facebook/php-sdk-v4": "~5.0",
        "guzzlehttp/guzzle": "~6.0",
        "imagine/imagine": "^0.6.3",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
